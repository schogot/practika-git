    "def addDigits(num: int) -> int:\n",
    "    if num == 0:\n",
    "        return 0\n",
    "    else:\n",
    "        return 1 + (num - 1) % 9\n",
    "\n",
    "# Example usage:\n",
    "print(addDigits(38))  # Output: 2\n",
    "print(addDigits(0))   # Output: 0\n",
    "print(addDigits(123)) # Output: 6\n"
   ]
